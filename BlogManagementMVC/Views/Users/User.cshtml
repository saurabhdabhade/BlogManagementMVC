@model BlogManagementLibrary.Model.User

<div class="bg-secondary bg-opacity-10 py-2">
    <div class=" container">
        <h1>New User</h1>
    </div>
</div>

<div class="container py-5">
    <form method="post">
        <div class="mb-3">
            <lable class="form-lable">User Name</lable>
            <input type="text" class="form-control" id="userName" name="userName" asp-for="UserName" required />
        </div>

        <div class="mb-3">
            <lable class="form-lable">User Email</lable>
            <input type="text" class="form-control" id="userEmail" name="userEmail" asp-for="UserEmail" required />
        </div>

        <div class=" mb-3">
            <button type="submit" class="btn btn-dark" asp-controller="Blog" asp-action="Blog">Login</button>
        </div>
    </form>
</div>